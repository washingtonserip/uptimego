{
  "family": "uptimego-api-task-definition",
  "containerDefinitions": [
    {
      "name": "uptimego-api-container",
      "image": "494755270253.dkr.ecr.us-east-2.amazonaws.com/uptimego_api:latest",
      "cpu": 256,
      "memory": 512,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "requires_compatibilities": ["FARGATE"],
  "network_mode": "awsvpc",
  "cpu": 256,
  "memory": 512,
  "execution_role_arn": "arn:aws:ecs:us-east-2:494755270253:service/uptimego-api-cluster/uptimego-api-service"
}
